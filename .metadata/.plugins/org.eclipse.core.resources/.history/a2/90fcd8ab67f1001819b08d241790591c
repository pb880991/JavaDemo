package 吃苹果比赛;

class Person extends Thread{
	private int num = 50;
	public void run() {
		for (int i = 0; i < 50; i++) {
			if(0 < num) {
				System.out.println(getName() + "吃了第" + num-- + "苹果");
			}	
		}
	}
}

public class ExtendsDemo {
	public static void main(String[] args) {
		
		new Person().start();
		new Person().start();
		new Person().start();
}
}
