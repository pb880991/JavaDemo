package _01;
/*并行：在同一时刻点发生
 *并发：在同一个时间段发生
 * */

public class GameDemo {

	public static void main(String[] args) {
		
		playGame();
		playMusic();
	}
	
	private static void playGame() {
		for (int i = 0; i < 1000; i++) {
			System.out.println("playGame----------");
		}
	}
	private static void playMusic() {
		for (int i = 0; i < 1000; i++) {
			System.out.println("playMusic");
		}
	}
}
