import java.util.Random;

//随机数
public class RandomDemo {
	
	
	
	public static void main(String[] args) {
		Random r1 = new Random(10);
		System.out.println(r1.nextBoolean());
		System.out.println(r1.nextBoolean());
		System.out.println(r1.nextBoolean());
		System.out.println(r1.nextBoolean());
	}
}
